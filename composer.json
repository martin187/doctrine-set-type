{
    "name": "martin187/doctrine-set-type",
    "description": "Set Type for Doctrine",
    "type": "library",
    "keywords": [
        "php",
        "doctrine",
        "doctrine-type",
        "doctrine-type-set"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Roma Baranenko",
            "email": "jungle.romabb8@gmail.com",
            "role": "Lead Developer"
        },
        {
            "name": "Martin Balcar",
            "email": "balcar.martin@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/jungle-bay/doctrine-set-type/issues/"
    },
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^3.5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Doctrine\\": "src/Doctrine/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Doctrine\\Tests\\": "tests/Doctrine/Tests/"
        }
    },
    "scripts": {
        "test": "phpunit --bootstrap vendor/autoload.php --filter",
        "tests": "phpunit --bootstrap vendor/autoload.php tests"
    },
    "archive": {
        "exclude": [
            "*",
            ".*",
            "!src/*",
            "!composer.json"
        ]
    }
}
