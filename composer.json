{
    "name": "jungle-bay/doctrine-set-type",
    "description": "Doctrine Set Type",
    "type": "library",
    "keywords": [
        "php",
        "doctrine",
        "set",
        "type"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Roma Baranenko",
            "email": "jungle.romabb8@gmail.com",
            "role": "Lead Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/jungle-bay/doctrine-set-type/issues/"
    },
    "require": {
        "php": ">=5.3.3",
        "doctrine/dbal": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Doctrine\\": "src/Doctrine/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Doctrine\\Tests\\": "tests/Doctrine/Tests/"
        }
    },
    "scripts": {
        "test": "phpunit --bootstrap vendor/autoload.php --filter",
        "tests": "phpunit --bootstrap vendor/autoload.php tests"
    },
    "archive": {
        "exclude": [
            "!vendor",
            "tests",
            "phpunit.xml",
            ".travis.yml",
            "composer.phar"
        ]
    }
}
